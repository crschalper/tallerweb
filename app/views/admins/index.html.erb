<h1>Listing admins</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Lastname</th>
    <th>Username</th>
    <th>Email</th>
    <th>Rut</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admins.each do |admin| %>
  <tr>
    <td><%=admin.name%></td>
    <td><%=admin.lastname%></td>
    <td><%=admin.username%></td>
    <td><%=admin.email%></td>
    <td><%=admin.rut%></td>
    <td><%= link_to 'Show', admin %></td>
    <td><%= link_to 'Edit', edit_user_path(admin) %></td>
    <td><%= link_to 'Destroy', admin, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

